{
  "name": "@piiano/vault-client",
  "version": "1.0.0",
  "description": "Piiano Vault generated typescript client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Ori Shalom <ori.shalom@piiano.com>",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "repository": "git://github.com/piiano/vault-typescript.git",
  "homepage": "https://github.com/piiano/vault-typescript#readme",
  "scripts": {
    "openapi": "curl https://piiano.com/docs/assets/openapi.yaml --output openapi.yaml",
    "clean": "rm -rf ./src",
    "generate": "openapi --input openapi.yaml --output ./src -c axios",
    "test": "TS_NODE_PROJECT='./tsconfig.test.json' mocha -c -r ts-node/register -r test/hooks.ts 'test/**/*.spec.ts'",
    "tsc": "tsc",
    "build": "yarn openapi && yarn clean && yarn generate && yarn tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@piiano/testcontainers-vault": "^1.0.0",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.10",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "openapi-typescript-codegen": "^0.23.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
